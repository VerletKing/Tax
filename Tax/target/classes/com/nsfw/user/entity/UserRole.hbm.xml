<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">



<hibernate-mapping package="com.nsfw.user.entity" auto-import="true">
	<class name="UserRole" table="userRole">
		<composite-id name="id" class="UserRoleId" >
			<key-many-to-one name="role" class="com.nsfw.role.entity.Role" lazy="false">
				<column name="role_id"></column>
			</key-many-to-one>
			<key-property name="userId" type="java.lang.String">
				<column name="user_id" length="32"></column>
			</key-property>
		</composite-id>
	</class>

</hibernate-mapping>
